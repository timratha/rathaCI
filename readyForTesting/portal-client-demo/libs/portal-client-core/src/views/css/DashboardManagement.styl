.DashboardManagement
  .breadCrumb //TODO consider make it common widget
    padding 10px 0;
    span
      vertical-align middle
      i
        padding 0 5px
    span.root
      font-size 2em
  .dashboardGrid
    height auto
    min-width 500px
    .dgrid-header
      width 100%
    .dgrid-scrollbar-width
      display none
      width 0
    .dgrid-scroller
      position relative
      overflow visible
    .field-iconClass
      width 70px
      text-align center
     .dgrid-cell
       vertical-align middle
       .actionsCont
        i
          padding-left 10px
          cursor pointer

