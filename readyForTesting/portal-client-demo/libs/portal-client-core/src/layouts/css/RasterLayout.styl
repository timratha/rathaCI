  position relative
  width: 100%;
  height: 100%;
  background-color inherit
  .grids
    width: 100%;
    height: 100%;
    border-collapse: collapse;
    .cell
      border:1px dashed rgba(130, 223, 250, 0.37);
      vertical-align: top;
      .wrapper .titleBarContainer
        cursor move ;
    .dojoDndContainerOver
